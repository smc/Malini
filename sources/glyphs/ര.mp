input glyph;
beginglyph;
z0=(x1 + 0.3mw, 0);
z1=(0, y0 + 0.5m);
z2=(x1 + mw/2, y1 + m/2);
z3=(x2 + mw/2, y1);
z4=(x3 - mw/3, y0);
z5=(x4 - mw/4, y4 + m/3);

paths[0] = z0..z1..z2{right}..z3..z4..{dir 90}z5;
z6 = point 2.6 of paths[0];
paths[0] := paths[0]..{right}z6 ;
pen_stroke(
    nib(start_up_nib)(0)
    nib(middle_up_nib)(1)
    nib(top_nib)(2)
    nib(middle_down_nib)(3)
    nib(down_nib)(4)
    nib(middle_up_nib)(5)
    nib(top_nib)(6)
)(paths[0])(strokes[0]);

endglyph;
