input glyph;
beginglyph;
input ./glyphs/include/à´£;

xshift := 0.6mw;
yshift := -m;

for i=1 upto 4:
  p:=p+1;
  paths[p] = paths[p-4] shifted (xshift , yshift) xscaled 0.8 yscaled 0.75;
  strokes[p] = strokes[p-4] shifted (xshift , yshift) xscaled 0.8 yscaled 0.75;
endfor;

rbearing:=0;
endglyph;
